package unite1;

public class alistirma10 {
	public static void main(String[] args) {
        double pay = (7.5 * 6.5) - (4.5 * 3);
        double payda = 47.5 - 5.5;

        double solution = pay / payda;

        System.out.printf("Solution : " + solution);

    }

}
  
  OUTPUT:
    
Solution : 0.8392857142857143
