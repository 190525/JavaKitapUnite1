
package unite1;
// Display a pattern
public class alistirma5 {
	public static void main(String[] args) {	
	
System.out.println("		J");
System.out.println( "	J 	aaa 	v 	vaaa");
System.out.println("	J 	J 	aa 	v 	v 	a 	a");
System.out.println("	J 	aaaa 	v 	aaaa");

	}
}
	OUTPUT:
		J
	J 	aaa 	v 	vaaa
	J 	J 	aa 	v 	v 	a 	a
	J 	aaaa 	v 	aaaa

